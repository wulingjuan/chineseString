page{
  background:#f3f3f3;
}
view{
  box-sizing: border-box;
}
image{
  width:100%;
  border-radius: 10rpx;
}
.red-square{
  width:100rpx;
  height:100rpx;
  background:url(data:image/png;base64,);
  background-size:100% 100%;
  position:fixed;
  bottom:200rpx;
  right:20rpx;
}
.container{
  padding:60rpx 20rpx;  
  background:#c96148;
} 
.logo{
  width:120rpx;
  height:120rpx;
}
.username{
  color:#fff6c7;
  font-size:32rpx;
}
.program-logo{
  margin-top:15rpx;
  margin-left:-9rpx;
  width:300rpx;
  height:300rpx;
}
.clock{
  padding:20rpx;
  color:#ffffe6;
  font-size:28rpx;
  text-align: center;
}
.money{
  color:#ffffe6;
  margin:120rpx 0rpx 20rpx;
  font-size:50rpx;
}
.fail{
  margin:80rpx 0rpx 20rpx;
  color:#fff;
}
.fail-title{
  font-size:50rpx;
  text-align: center;
  margin-bottom: 20rpx;
}
.fail-hint{
  font-size:28rpx;text-align: center;
}
.four-words{
  text-align: center;
  padding:20rpx 40rpx;
  color:#c96148;
  border-radius:20rpx;
  background:url(data:image/png;base64,);
  background-size:100% 100%;
}
.input{
  padding:10rpx 20rpx;
  width:500rpx;
  background:#fbfec5;
  color:#807c7b;
  font-size:28rpx;
  border-radius:40rpx;
  margin-top:120rpx;
}
.input input{
  text-align:center;
}
.btn{
  background:#51a938;
  width:50%;
  color:#fff;
  font-size:28rpx;
  margin-top:20rpx;
  border:none;
}
.icon{
  display:flex;
  flex-direction:row;
  width:640rpx;
  margin:20rpx auto;
}
.icon-item{
  flex:1;
  color:#ffefe4;
  font-size:32rpx;
  overflow:hidden;
}
.icon-item navigator{
  width:100%;
}
.icon-item-img{
  width:36rpx;
  float:left;
  margin-right:10rpx;
}
.icon-item-img image{
  width:100%;
}
.icon-item-txt{
  float:left;
  line-height:32rpx;
}
.list{
  overflow:hidden;
  font-size:32rpx;
  padding:20rpx 0rpx;
}
.list-wallet{
  float:left;
}
.list-complain{
  float:right;
}
.list-item{
  display: flex;
  flex-direction: row;
  background:#fff;
  margin-bottom:20rpx;
  padding:10rpx 20rpx;
}
.list-item-img{
  width:80rpx;
}
.list-item-img image{
  width:100%;
}
.list-item-info{
  color:#666;
  font-size:28rpx;
  margin-left:20rpx;
  width:445rpx;
}
.list-item-money{
  color:#666;
  font-size:28rpx;
}
.try{
  padding-top:80rpx;
  font-size:28rpx;
  color:#666;
  text-align:center;
  margin-bottom: 100rpx;
}
.try navigator{
  display:inline;
}
