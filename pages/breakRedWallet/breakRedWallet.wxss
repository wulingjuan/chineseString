page{
  background:#f3f3f3;
}
view{
  box-sizing: border-box;
}
image{
  width:100%;
  border-radius: 10rpx;
}
.red-square{
  width:100rpx;
  height:100rpx;
  background:url(data:image/png;base64,);
  background-size:100% 100%;
  position:fixed;
  bottom:200rpx;
  right:20rpx;
}
.container{
  padding:50rpx 20rpx;  
  background:#d45149;
} 
.logo{
  width:120rpx;
  height:120rpx;
}
.username{
  color:#f7c7ba;
  font-size:32rpx;
  margin-top: 5rpx;
}
.program-logo{
  margin-top:15rpx;
  margin-left:-9rpx;
  width:300rpx;
  height:300rpx;
}
.clock{
  padding:10rpx 0rpx;
  color:#f7c7ba;
  font-size:28rpx;
  text-align: center;
}
.money{
  color:#ffffe6;
  margin:80rpx 0rpx 20rpx;
  font-size:50rpx;
}
.fail{
  margin:80rpx 0rpx 20rpx;
  color:#fff;
}
.fail-title{
  font-size:50rpx;
  text-align: center;
  margin-bottom: 20rpx;
}
.fail-hint{
  font-size:28rpx;
  text-align: center;
}
.four-words{
  text-align: center;
  padding:20rpx 40rpx;
  color:#ffe2b1;
  font-size: 40rpx;
}
.input{
  padding:10rpx 20rpx;
  width:500rpx;
  color:#807c7b;
  font-size:28rpx;
  background: #fff;
  margin-top:60rpx;
  border-radius: 8rpx;
}
.input input{
  text-align:center;
}
.btn{
  background:#ffe2b1;
  width:100%;
  color:#666;
  font-size:28rpx;
  margin:20rpx 0rpx;
  padding: 4rpx 0rpx;
  border:none;
  border-radius: 50rpx;
  margin-top: 40rpx;
}
.icon{
  display:flex;
  flex-direction:row;
  width:650rpx;
  margin:20rpx auto;
}
.icon-item{
  color:#ffffff;
  font-size:28rpx;
  overflow:hidden;
  margin-left: 50rpx;
}
.icon-item navigator{
  width:100%;
}
.icon-item-img{
  width:36rpx;
  float:left;
  margin-right:10rpx;
}
.icon-item-img image{
  width:100%;
}
.icon-item-txt{
  float:left;
  line-height:32rpx;
}
.list{
  overflow:hidden;
  font-size:28rpx;
  padding:20rpx 0rpx;
}
.list-wallet{
  float:left;
}
.list-complain{
  float:right;
  color: #586c94;
  font-size: 28rpx;
}
/*广告位 */
.adver{
  position: relative;
  margin: 0rpx auto 40rpx;
  display: flex;
  width: 680rpx;
  height: 300rpx;
  background: #fff;
  padding: 20rpx; 
}
.adver-logo{
  position: absolute;
  background: rgba(0,0,0,0.4);
  top: 20rpx;
  right: 20rpx;
  padding: 10rpx 20rpx;
  color: #fff;
  border-radius: 8rpx;
}
.adver-img{
  width: 200rpx;
  height: 200rpx;
  margin-top: 30rpx; 
   margin-right: 40rpx;  
}
.adver-img img{
  width: 100%;
}
.adver-txt{
  width: 360rpx;
  margin-left: 40rpx; 
}
.adver-title{
  margin-top: 20rpx;
}
.adver-content{
  margin-top: 20rpx;
}
.adver-join{
  margin-top: 20rpx;
  color: #d14e53;
}
.list-item{
  display: flex;
  flex-direction: row;
  background:#fff;
  margin-bottom:20rpx;
  padding:10rpx 20rpx;
}
.list-item-img{
  width:80rpx;
}
.list-item-img image{
  width:100%;
}
.list-item-info{
  color:#666;
  font-size:28rpx;
  margin-left:20rpx;
  width:445rpx;
}
.list-item-money{
  color:#666;
  font-size:28rpx;
}
.try{
  padding:80rpx 0rpx 20rpx; 
  font-size:24rpx;
  width: 240rpx;
  margin: 0rpx auto;
  color: #586c94;
}
.try navigator{
  display:inline;
}
.try navigator:nth-child(1){
  margin-right: 10rpx;
}
.try navigator:nth-child(2){
  margin-left: 10rpx;
}